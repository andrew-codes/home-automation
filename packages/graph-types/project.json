{
  "root": "packages/graph-types",
  "sourceRoot": "packages/graph-types/src",
  "projectType": "library",
  "targets": {
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["src/generated"],
      "options": {
        "command": "rm -rf src/generated .test-runs",
        "cwd": "packages/graph-types"
      }
    },

    "generate": {
      "executor": "@ha/nx-executors:invoke",
      "outputs": ["src/index.d.ts"],
      "options": {
        "module": "scripts/generate.ts",
        "cwd": "packages/graph-types"
      },
      "dependsOn": ["clean"]
    }
  },
  "tags": [],
  "implicitDependencies": ["subgraph-gaming", "graph-subscriptions"]
}
