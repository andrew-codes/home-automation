{
  "root": "packages/github-secrets",
  "sourceRoot": "packages/github-secrets/src",
  "projectType": "library",
  "targets": {
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [".test-runs/unit"],
      "options": {
        "command": "rm -rf dist .test-runs",
        "cwd": "packages/github-secrets"
      }
    },
    "compile": {
      "executor": "@ha/nx-executors:invoke",
      "outputs": ["packages/github-secrets/dist"],
      "options": {
        "module": "scripts/compile.ts",
        "cwd": "packages/github-secrets"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [".test-runs/unit"],
      "options": {
        "command": "jest",
        "cwd": "packages/github-secrets"
      }
    },
    "code-coverage": {
      "executor": "@ha/nx-executors:upload-codecov",
      "options": {
        "coverageFilePath": "packages/github-secrets/.test-runs/unit/lcov.info"
      }
    }
  },
  "tags": [],
  "implicitDependencies": []
}
