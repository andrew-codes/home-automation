{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "cli": "nx",
  "properties": {
    "port": {
      "type": "number",
      "description": "Local port."
    },
    "serviceName": {
      "type": "string",
      "description": "Override service name (defaults to project name)."
    },
    "volumes": {
      "type": "array",
      "description": "Volume mappings"
    },
    "envOverrides": {
      "type": "array",
      "description": "Volume mappings"
    },
    "pull": {
      "type": "boolean",
      "description": "Pull image before running."
    }
  }
}
