{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "cli": "nx",
  "properties": {
    "port": {
      "type": "number",
      "description": "Local port."
    },
    "command": {
      "type": "string",
      "description": "Command to run locally."
    },
    "cwd": {
      "type": "string",
      "description": "CWD to execute command."
    },
    "serviceName": {
      "type": "string",
      "description": "Override service name (defaults to project name)."
    }
  }
}
