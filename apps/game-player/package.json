{
  "private": true,
  "name": "@ha/game-player-app",
  "version": "0.0.1",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "concurrently 'cp src/server.js dist/index.js' 'webpack --config webpack.config.js'",
    "image/push": "docker run -v $PWD:/workspace gcr.io/kaniko-project/executor:latest --dockerfile /workspace/Dockerfile --destination \"$DOCKER_REGISTRY_DOMAIN:5000/game-player:latest\" --context dir:///workspace/ --insecure",
    "start": "scripts/bin/start.sh",
    "start/dev": "cross-env NODE_ENV=development nodemon src/server.ts --inspect=0.0.0.0:9233"
  },
  "dependencies": {
    "apollo-fetch": "^0.7.0",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.0.0",
    "ts-loader": "^8.0.15",
    "webpack": "^5.21.1",
    "webpack-dev-middleware": "^4.1.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.8",
    "@ha/ts-config": "^0.0.1",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@zeit/ncc": "^0.22.3",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "graphql": "^15.5.0",
    "nodemon": "^2.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack-cli": "^4.5.0"
  }
}
