{
  "root": "apps/secrets",
  "sourceRoot": "apps/secrets",
  "projectType": "application",
  "targets": {
    "compile": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "apps/secrets",
        "module": "scripts/compile.ts"
      }
    },
    "deploy": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "apps/secrets",
        "module": "scripts/deploy.ts"
      },
      "dependsOn": [
        { "target": "compile", "projects": "self" },
        {
          "target": "deploy",
          "projects": "dependencies"
        }
      ]
    },
    "initialize-secrets": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "apps/secrets",
        "module": "scripts/initialize-secrets.ts"
      }
    },
    "template": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "apps/secrets",
        "module": "scripts/template-secrets-env-file.ts"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["deployment-utils"]
}
