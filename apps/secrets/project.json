{
  "root": "apps/secrets",
  "sourceRoot": "apps/secrets",
  "projectType": "application",
  "targets": {
    "compile": {
      "executor": "@ha/nx-executors:invoke",
      "outputs": ["apps/secrets/dist"],
      "options": {
        "cwd": "apps/secrets",
        "module": "scripts/compile.ts"
      }
    },
    "deploy": {
      "executor": "@ha/nx-executors:run-with-az",
      "options": {
        "cwd": "apps/secrets",
        "command": "./scripts/bin/deploy.sh"
      },
      "dependsOn": [{ "target": "compile", "projects": "self" }]
    },
    "initialize-secrets": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "apps/secrets",
        "module": "scripts/initialize-secrets.ts"
      }
    }
  },
  "tags": [],
  "implicitDependencies": []
}
