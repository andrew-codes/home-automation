{
  "private": true,
  "name": "@ha/guest-wifi-renewal-app",
  "version": "0.0.1",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "ncc build src/index.ts -o dist -m",
    "image/push": "docker run -v $PWD:/workspace gcr.io/kaniko-project/executor:latest --dockerfile /workspace/Dockerfile --destination \"docker-registry:5000/guest-wifi-renewal:latest\" --context dir:///workspace/ --insecure --cache=true",
    "start/dev": "nodemon src/index.ts --inspect=0.0.0.0:9233",
    "start": "scripts/bin/start.sh"
  },
  "dependencies": {
    "async-mqtt": "^2.6.1",
    "debug": "^4.3.1",
    "home-assistant-js-websocket": "^5.10.0",
    "node-unifiapi": "^0.0.54",
    "ws": "^8.2.2"
  },
  "devDependencies": {
    "@ha/ts-config": "^0.0.1",
    "@zeit/ncc": "^0.22.3",
    "jest": "^27.0.0",
    "jest-when": "^3.2.1",
    "nodemon": "^2.0.6",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  }
}
