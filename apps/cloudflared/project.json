{
  "name": "cloudflared",
  "sourceRoot": "{projectRoot}/src",
  "projectType": "application",
  "targets": {
    "deploy": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "{projectRoot}",
        "module": "scripts/deploy.ts"
      }
    },
    "clean/deploy": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "{projectRoot}",
        "module": "rm -rf .terraform src/.terraform* src/provision/terraform* src/provision/.terraform* src/provision/terraform.plan"
      }
    }
  },
  "tags": [],
  "implicitDependencies": []
}
