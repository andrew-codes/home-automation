{
  "root": "apps/graph",
  "sourceRoot": "apps/graph/src",
  "projectType": "application",
  "targets": {
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "rm -rf dist",
        "cwd": "apps/graph"
      }
    },
    "prepare": {
      "executor": "@ha/nx-executors:invoke",
      "outputs": ["apps/graph/router"],
      "options": {
        "module": "scripts/prepare.ts",
        "cwd": "apps/graph"
      }
    },
    "deploy": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "apps/graph",
        "module": "scripts/deploy.ts"
      },
      "dependsOn": [
        {
          "target": "image/push",
          "projects": "self"
        }
      ]
    },
    "image/push": {
      "executor": "@ha/nx-executors:invoke",
      "dependsOn": [
        {
          "target": "compile",
          "projects": "self"
        }
      ],
      "options": {
        "module": "scripts/image-push.ts",
        "cwd": "apps/graph"
      }
    },
    "compile": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "module": "scripts/compile.ts",
        "cwd": "apps/graph"
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["node --require esbuild-register scripts/nodemon.ts"],
        "cwd": "apps/graph"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["subgraph-gaming"]
}
