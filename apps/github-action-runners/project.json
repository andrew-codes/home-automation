{
  "root": "apps/github-action-runners",
  "sourceRoot": "apps/github-action-runners/src",
  "projectType": "application",
  "targets": {
    "deploy": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "apps/github-action-runners",
        "module": "scripts/deploy.ts"
      },
      "dependsOn": [
        {
          "target": "compile",
          "projects": "dependencies"
        },
        {
          "target": "image/push",
          "projects": "self"
        }
      ]
    },
    "image/push": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "module": "scripts/image-push.ts",
        "cwd": "apps/github-action-runners"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["secrets", "github-secrets", "docker-creds"]
}
