{
  "private": true,
  "name": "@ha/guest-pin-codes-app",
  "version": "0.0.1",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist .test-runs",
    "compile": "node -r esbuild-register build.ts",
    "image/push": "docker run -v $PWD:/workspace gcr.io/kaniko-project/executor:latest --dockerfile /workspace/Dockerfile --destination \"docker-registry:5000/guest-pin-codes:latest\" --context dir:///workspace/ --insecure --cache=true",
    "start/dev": "nodemon src/index.ts",
    "start": "scripts/bin/start.sh",
    "test": "jest"
  },
  "nx": {
    "targets": {
      "clean": {
        "outputs": [
          "dist"
        ]
      },
      "compile": {
        "outputs": [
          "dist"
        ]
      },
      "image/push": {
        "dependsOn": [
          {
            "target": "compile",
            "projects": "self"
          }
        ]
      },
      "test": {
        "outputs": [
          ".test-runs/unit"
        ]
      }
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@ha/configure-jest": "^0.0.1",
    "@ha/mqtt-client": "^0.0.1",
    "@ha/unifi-client": "^0.0.1",
    "@redux-saga/is": "^1.0.1",
    "@redux-saga/symbols": "^1.0.1",
    "async-mqtt": "^2.6.1",
    "cron": "^1.8.2",
    "debug": "^4.3.1",
    "esbuild-register": "^3.3.2",
    "google-auth-library": "^7.0.4",
    "googleapis": "^92.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.6",
    "redux-saga-test-plan": "^4.0.3",
    "redux-saga": "^1.1.3",
    "redux": "^4.0.5",
    "reselect": "^4.1.5"
  }
}
