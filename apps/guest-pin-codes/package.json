{
  "private": true,
  "name": "@ha/guest-pin-codes-app",
  "version": "0.0.1",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "ncc build src/index.ts -o dist -m",
    "image/push": "docker run -v $PWD:/workspace gcr.io/kaniko-project/executor:latest --dockerfile /workspace/Dockerfile --destination \"$DOCKER_REGISTRY_DOMAIN:5000/guest-pin-codes:latest\" --context dir:///workspace/ --insecure --cache=true",
    "start/dev": "nodemon src/index.ts --inspect=0.0.0.0:9233",
    "start": "scripts/bin/start.sh"
  },
  "dependencies": {
    "async-mqtt": "^2.6.1",
    "cron": "^1.8.2",
    "debug": "^4.3.1",
    "google-auth-library": "^7.0.4",
    "googleapis": "^77.0.0",
    "lodash": "^4.17.21",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@ha/ts-config": "^0.0.1",
    "@zeit/ncc": "^0.22.3",
    "nodemon": "^2.0.6",
    "redux-saga-tester": "^1.0.874",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  }
}
