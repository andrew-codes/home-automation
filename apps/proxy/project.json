{
  "root": "apps/proxy",
  "sourceRoot": "apps/proxy",
  "projectType": "application",
  "targets": {
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/proxy",
        "command": "rm -rf dist"
      }
    },
    "compile": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "apps/proxy",
        "module": "scripts/compile.ts"
      },
      "dependsOn": [{ "target": "clean", "projects": "self" }]
    },
    "deploy": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "apps/proxy",
        "module": "scripts/deploy.ts"
      },
      "dependsOn": [
        { "target": "compile", "projects": "self" },
        {
          "target": "deploy",
          "projects": "dependencies"
        }
      ]
    }
  },
  "tags": [],
  "implicitDependencies": []
}
