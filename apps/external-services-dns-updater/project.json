{
  "root": "apps/external-services-dns-updater",
  "sourceRoot": "apps/external-services-dns-updater/src",
  "projectType": "application",
  "targets": {
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["dist", ".test-runs"],
      "options": {
        "command": "rm -rf dist .test-runs",
        "cwd": "apps/external-services-dns-updater"
      }
    },
    "compile": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["apps/external-services-dns-updater/dist"],
      "options": {
        "command": "node -r esbuild-register build.ts",
        "cwd": "apps/external-services-dns-updater"
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/external-services-dns-updater",
        "command": "./scripts/bin/deploy.sh"
      },
      "dependsOn": [
        {
          "target": "image/push",
          "projects": "self"
        }
      ]
    },
    "image/push": {
      "executor": "@nrwl/workspace:run-commands",
      "dependsOn": [
        {
          "target": "compile",
          "projects": "self"
        }
      ],
      "options": {
        "command": "docker run -v $PWD:/workspace gcr.io/kaniko-project/executor:latest --dockerfile /workspace/Dockerfile --destination \"docker-registry:5000/external-services-dns-updater:latest\" --context dir:///workspace/ --insecure --cache=true",
        "cwd": "apps/external-services-dns-updater"
      }
    },
    "serve": {
      "executor": "@ha/nx-executors:telepresence",
      "dependsOn": [
        {
          "target": "compile",
          "projects": "dependencies"
        }
      ]
    },
    "start/dev": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "nodemon src/index.ts --inspect=0.0.0.0:9233",
        "cwd": "apps/external-services-dns-updater"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [".test-runs/unit"],
      "options": {
        "command": "jest",
        "cwd": "apps/external-services-dns-updater"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["mqtt", "secrets"]
}
