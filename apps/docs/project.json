{
  "name": "docs",
  "sourceRoot": "{projectRoot}/src",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "yarn gatsby develop"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "yarn gatsby clean"
      }
    },
    "compile": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{projectRoot}",
        "command": "yarn gatsby build"
      },
      "cache": false
    },
    "package": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "{projectRoot}",
        "module": "./scripts/package.ts"
      }
    },
    "publish": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "{projectRoot}",
        "module": "./scripts/publish.ts"
      }
    },
    "deploy": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "{projectRoot}",
        "module": "./scripts/deploy.ts"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["k8s", "docker-creds", "cloudflared"]
}
