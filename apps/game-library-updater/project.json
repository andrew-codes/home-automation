{
  "root": "apps/game-library-updater",
  "sourceRoot": "apps/game-library-updater/src",
  "projectType": "application",
  "targets": {
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["dist", ".test-runs"],
      "options": {
        "command": "rm -rf dist .test-runs",
        "cwd": "apps/game-library-updater"
      }
    },
    "compile": {
      "executor": "@ha/nx-executors:invoke",
      "outputs": ["apps/game-library-updater/dist"],
      "options": {
        "module": "scripts/compile.ts",
        "cwd": "apps/game-library-updater"
      }
    },
    "deploy": {
      "executor": "@ha/nx-executors:invoke",
      "options": {
        "cwd": "apps/game-library-updater",
        "module": "scripts/deploy.ts"
      },
      "dependsOn": [
        {
          "target": "image/push",
          "projects": "self"
        },
        {
          "target": "deploy",
          "projects": "dependencies"
        }
      ]
    },
    "image/push": {
      "executor": "@ha/nx-executors:invoke",
      "dependsOn": [
        {
          "target": "compile",
          "projects": "self"
        }
      ],
      "options": {
        "module": "scripts/image-push.ts",
        "cwd": "apps/game-library-updater"
      }
    },
    "serve": {
      "executor": "@ha/nx-executors:telepresence-local",
      "options": {
        "fromPort": 8083,
        "toPort": 80,
        "cwd": "apps/game-library-updater",
        "command": "yarn nodemon --inspect src/index.ts"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [".test-runs/unit"],
      "options": {
        "command": "jest",
        "cwd": "apps/game-library-updater"
      }
    },
    "code-coverage": {
      "executor": "@ha/nx-executors:upload-codecov",
      "options": {
        "coverageFilePath": "apps/game-library-updater/.test-runs/unit/lcov.info"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["gaming-assets-shared-volume"]
}
