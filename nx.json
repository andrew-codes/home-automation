{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "compile",
          "generate",
          "lint",
          "prepare",
          "prestart",
          "test",
          "package"
        ]
      }
    }
  },
  "npmScope": "@ha",
  "extends": "nx/presets/npm.json",
  "affected": {
    "defaultBase": "main"
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "packages"
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "clean/deploy": {
      "dependsOn": [
        "^prepare",
        "prepare",
        "^clean/deploy",
        "^clean/provision",
        "clean/provision"
      ]
    },
    "compile": {
      "dependsOn": ["^prepare", "prepare", "^generate", "generate", "^compile"]
    },
    "deploy": {
      "dependsOn": [
        "^prepare",
        "prepare",
        "compile",
        "^provision",
        "^deploy",
        "provision",
        "publish"
      ]
    },
    "package": {
      "dependsOn": ["compile"]
    },
    "publish": {
      "dependsOn": ["^package", "package", "^publish", "^provision", "^deploy"]
    },
    "provision": {
      "dependsOn": ["^prepare", "prepare", "^provision", "^deploy"]
    },
    "generate": {
      "dependsOn": ["^prepare", "prepare", "^generate"]
    },
    "image/push": {
      "dependsOn": ["^prepare", "prepare", "compile"]
    },
    "lint": {
      "dependsOn": ["^prepare", "prepare"]
    },
    "prepare": {
      "dependsOn": ["^prepare"]
    },
    "start": {
      "dependsOn": ["^prepare", "prepare", "^generate", "generate", "prestart"]
    },
    "serve": {
      "dependsOn": ["^prepare", "prepare", "^generate", "generate", "prestart"]
    },
    "test": {
      "dependsOn": ["^prepare", "prepare"]
    }
  }
}
