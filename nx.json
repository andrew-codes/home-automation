{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "clean",
          "compile",
          "deploy",
          "generate",
          "image/push",
          "lint",
          "prepare",
          "prestart",
          "test"
        ]
      }
    }
  },
  "npmScope": "@ha",
  "extends": "nx/presets/npm.json",
  "affected": {
    "defaultBase": "main"
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "packages"
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "compile": {
      "dependsOn": ["^prepare", "prepare", "^generate", "generate", "^compile"]
    },
    "deploy": {
      "dependsOn": ["^prepare", "prepare", "compile", "image/push"]
    },
    "generate": {
      "dependsOn": ["^prepare", "prepare", "^generate"]
    },
    "image/push": {
      "dependsOn": ["^prepare", "prepare", "compile"]
    },
    "lint": {
      "dependsOn": ["^prepare", "prepare"]
    },
    "prepare": {
      "dependsOn": ["^prepare"]
    },
    "serve": {
      "dependsOn": ["^prepare", "prepare", "^generate", "generate", "prestart"]
    },
    "test": {
      "dependsOn": ["^prepare", "prepare"]
    }
  }
}
